    function getHolidayCount(startDate, endDate) {

        var holidayDate = null;
        var formattedHolidayList = [];
        var l = arrListHoliday.length;
        var cnt = 0;
        for (var ii = 0; ii < l; ii++) {
            for (var day = startDate; day <= endDate; day = (moment(day).add(1, 'days').format('YYYY-MM-DD'))) {
                if (day == moment(arrListHoliday[ii].HolidayDate).format('YYYY-MM-DD')) {
                    holidayDate = moment(arrListHoliday[ii].HolidayDate).format('YYYY-MM-DD');
                    formattedHolidayList.push(holidayDate);
                    cnt++;
                }
            }
        }
        if (cnt > 0) {
            var endDate1 = (moment(endDate).add(1, 'days').format('YYYY-MM-DD'));
            var endDate2 = (moment(endDate).add(cnt, 'days').format('YYYY-MM-DD'));
            for (var ii2 = 0; ii2 < l; ii2++) {
                for (var day1 = endDate1; day1 <= endDate2; day1 = (moment(day1).add(1, 'days').format('YYYY-MM-DD'))) {
                    if (day1 == moment(arrListHoliday[ii2].HolidayDate).format('YYYY-MM-DD')) {
                        holidayDate = moment(arrListHoliday[ii2].HolidayDate).format('YYYY-MM-DD');
                        formattedHolidayList.push(holidayDate);
                        cnt++;
                    }
                }
            }
        }

        return cnt;
    }

